generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Product {
  id          Int      @id @default(autoincrement())
  name        String
  category    String
  description String
  price       Float
  stockCount  Int
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
